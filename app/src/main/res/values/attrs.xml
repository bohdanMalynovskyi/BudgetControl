<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DateIntervalView">
        <attr name="startDate" format="string" />
        <attr name="endDate" format="string" />
    </declare-styleable>

    <declare-styleable name="IncomeCostsCounterView">
        <attr name="isIncome" format="boolean" />
        <attr name="value" format="float" />
    </declare-styleable>

    <declare-styleable name="BaseEditTextView">
        <attr name="inputType">
            <enum name="text" value="0" />
            <enum name="number" value="1" />
        </attr>
    </declare-styleable>
</resources>